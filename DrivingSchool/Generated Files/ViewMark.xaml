<Page
    x:Class="DrivingSchool.ViewMark"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:DrivingSchool"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <TextBlock x:ConnectionId='1' x:Name="HistoryBlock" HorizontalAlignment="Left" Margin="434,442,0,0" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <TextBlock x:ConnectionId='2' x:Name="IDBlock" Visibility="Collapsed" HorizontalAlignment="Left" Margin="10,43,0,0" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <ProgressBar x:ConnectionId='3' x:Name="MarkBar" HorizontalAlignment="Left" Height="28" Margin="399,389,0,0" VerticalAlignment="Top" Width="98"/>
    </Grid>
</Page>

