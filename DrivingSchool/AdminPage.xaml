<Page
    x:Class="DrivingSchool.AdminPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:DrivingSchool"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <Button x:Name="UplodeScore" Content="上传成绩" HorizontalAlignment="Left" Margin="91,200,0,0" VerticalAlignment="Top" Click="UplodeScore_Click"/>
        <Button x:Name="AddTeacherTwo" Content="添加二科教练" HorizontalAlignment="Left" Margin="91,250,0,0" VerticalAlignment="Top" Click="AddTeacherTwo_Click"/>
        <Button x:Name="AddTeacherThree" Content="添加三科教练" HorizontalAlignment="Left" Margin="250,250,0,0" VerticalAlignment="Top" Click="AddTeacherThree_Click"/>

        <Button x:Name="EditTeacher" Content="修改教练" HorizontalAlignment="Left" Margin="91,300,0,0" VerticalAlignment="Top"/>
        <Button x:Name="DeleteTeacher" Content="删除教练" HorizontalAlignment="Left" Margin="91,350,0,0" VerticalAlignment="Top"/>
    </Grid>
</Page>
